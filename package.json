{
  "name": "@lacussoft/cpf-fmt",
  "version": "1.0.6",
  "description": "Basic function to format CPF (Brazilian ID document).",
  "author": "Julio L. Muller",
  "license": "MIT",
  "keywords": [
    "lacussoft",
    "cpf",
    "format",
    "formatting",
    "formatar",
    "mascara",
    "br"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/cpf-fmt.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/cpf-fmt/issues"
  },
  "homepage": "https://github.com/juliolmuller/cpf-fmt#readme",
  "typings": "build/types.d.ts",
  "main": "build/index.js",
  "scripts": {
    "build:browser": "rm -rf dist/ && webpack --mode=production",
    "build:node": "rm -rf build/ && babel src/ --out-dir build/ && cp src/*.d.ts build/",
    "build": "npm run build:browser && npm run build:node",
    "lint": "eslint --fix src/**/*.js",
    "test": "jest"
  },
  "dependencies": {
    "@lacussoft/num-only": "^1.0.6",
    "html-escaper": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@lacussoft/eslint-config": "^1.0.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  }
}
